{
  "name": "fakerli-blog-vue-ssr",
  "version": "0.0.1",
  "description": "A Vue.js project with vue 2.0, vue-router and vuex for server side rendering.",
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack2",
    "server-side"
  ],
  "author": {
    "name": "authurli",
    "url": "https://www.fakerli.com",
    "email": "lc11235@qq.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run static && node ./server",
    "start": "cross-env NODE_ENV=production node ./server",
    "build": "npm run static && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.server.config.js --progress --hide-modules",
    "static": "node ./build/copy",
    "devNoServer": "npm run static && npm run devNoServer:client && npm run devNoServer:server",
    "devNoServer:client": "cross-env NODE_ENV=development webpack --mode development --config build/webpack.client.config.js",
    "devNoServer:server": "cross-env NODE_ENV=development webpack --mode development --config build/webpack.server.config.js"

  },
